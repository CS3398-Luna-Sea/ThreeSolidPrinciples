package threesolid;
public interface IEat {

    public void eat();

}
public interface IWork {
    public void work();
}
public class Manager implements IWork, IEat{
    public void work() {
        // ....lol jk i'm a manager
    }

    public void eat() {
        //.... eating in lunch break
    }

}
public class Robot implements IWork{
    public void work() {
        // ....working for free
    }



}
public class SuperWorker implements IWork, IEat{
    public void work() {
        // ....working super hard
    }

    public void eat() {
        //.... eating in lunch break
    }
}
public class main {

    public static Manager tsManager = new Manager();

    // The entry main() method
    public static void main(String[] args)
    {

        try
        {
            System.out.format("Starting ... \n");
        }
        catch (Exception main_except)
        {
            main_except.printStackTrace();
        }

        try
        {
            System.out.format("Stopping ... \n");
        }
        catch (Exception main_except)
        {
            main_except.printStackTrace();
        }

        System.exit(0);

    }
}

public class Worker implements IWork, IEat{
    public void work() {
        // ....working
    }

    public void eat() {
        //.... eating in lunch break
    }

}
